<% content_for :header_tags do %>
  <script type="text/javascript">//<![CDATA[
  let ActualDates = {
    original_dates: <%= original_dates.to_json.html_safe %>,
    actual_dates: <%= actual_dates.to_json.html_safe %>,
    cf_id: {
      start: <%= actual_start_date_cf_id %>,
      end: <%= actual_end_date_cf_id %>,
    },
    label_date_information: '<%= l(:label_date_information) %>',
    bar_settings: <%= bar_settings.to_json.html_safe %>,
  };
  //]]</script>
  <%= javascript_include_tag('actual_dates.js', plugin: 'redmine_actual_date') %>
  <% if Setting.plugin_redmine_actual_date[:rearrange_date_information_on_issue] %>
    <%= javascript_include_tag('rearrange_date_information.js', plugin: 'redmine_actual_date') %>
  <% end %>
<% end %>
